    "knex": "^0.21.0",
    "morgan": "^1.10.0",
    "pg": "^8.0.2",
    "uuid": "^8.0.0",
    "uuidv4": "^6.0.8",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
